@model Models.OutCome
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>新增出库</title>
    <link href="~/Content/Login/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/Login/css/animate.css" rel="stylesheet" />
    <link href="~/Content/Login/css/responsive.css" rel="stylesheet" />
    <link href="~/Content/Login/css/style.css" rel="stylesheet" />
    <link href="~/Content/Login/css/responsive.css" rel="stylesheet" />
    <link href="~/Content/Login/css/bootstrap.min.css" rel="stylesheet" />

    <script src="~/Content/Login/js/jquery.js"></script>
    <script src="~/Content/Login/js/modernizr.min.js"></script>
    <script src="~/Content/Login/js/bootstrap.min.js"></script>
    <script src="~/Content/Login/js/bootsnav.js"></script>
    <script src="~/Content/Login/js/jquery.sticky.js"></script>
    <script src="~/Content/Login/js/jquery.easing.min.js"></script>
    <script src="~/Content/Login/js/custom.js"></script>
    <script type="text/javascript">
        function add() {
            $.ajax({
                url: "@Url.Action("OutCome_add")",
                type: "post",
                data: $("form").serialize(),
                dataType: "json",
                success: function (data) {
                    data = eval("(" + data + ")");
                    if (data) {
                        alert("添加成功");
                        parent.closeWin();
                        parent.openFrame("Inventory_list");
                    } else {
                    }
                }
            });
        }
    </script>
</head>
<body>
    <section class="signin popup-in">
        <div class="container">
            <div class="sign-content popup-in-content">
                <div class="popup-in-txt">
                    <h2>新增出库</h2>
                    <div class="card">
                        <div class="card-body">
                            <div style="width:100%;text-align:center">
                                <div class="row">
                                    <div class="col-12">
                                        <form method="post" action="/Inventory/Inventory_list" style="width:100%;text-align:left">
                                            <div class="form-group mb-3">
                                                <table contenteditable="inherit" style="width:100%;text-align:left">
                                                    <tr>
                                                        <td>
                                                            <label for="example-date" style="width:100%;text-align:left">出库时间</label>
                                                            <input class="form-control" id="date" type="date" name="date" style="width:200px">
                                                        </td>
                                                       
                                                    
                                                        <td>
                                                            <label style="width:100%;text-align:left">材料名称查询</label>
                                                            <input id="nameinput" name="nameinput" list="namelist" style="width:200px;height:40px" class="form-control" placeholder="请选择或输入名称" />
                                                            <datalist id="namelist">
                                                                @*@foreach (var name in names)
                                                    {
                                                        <option>@name.materialName</option>
                                                    }*@
                                                            </datalist>
                                                        </td>
                                                   
                                                        <td>
                                                            <label style="width:100%;text-align:left">货品编号查询</label>
                                                            <input id="codeinput" name="codeinput" list="codelist" style="width:200px;height:40px" class="form-control" placeholder="请选择或输入编号" />
                                                            <datalist id="codelist">
                                                                @*@foreach (var emp in material)
                {
                    <option>@emp.code</option>
                }*@
                                                            </datalist>
                                                        </td>

                                                   
                                                        <td>
                                                            <label for="example-date" style="width:100%;text-align:left">结束时间</label>
                                                            <input class="form-control" id="date1" type="date" name="date1" style="width:200px">
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <div class="button-list">
                                                                <button type="submit" style="width:60px;height:50px; float:unset" class="btn btn-primary"
                                                                        data-toggle="modal" data-target=".signin_modal">
                                                                    查询
                                                                </button>
                                                            </div>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>
</html>
