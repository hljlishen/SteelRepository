    <script type="text/javascript">
        function GotoPartView(actionname, controllerName) {
            var url = "/" + controllerName + "/" + actionname;
            $.ajax({
                type: "post",
                url: url,
                success: function (data) {

                  //获取到数据后在右侧Content下显示分部视图
                    $('#bodycontent').html(data);
                }
            });
        }

